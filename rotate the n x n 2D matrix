class Solution {
    public void rotate(int[][] matrix) {
        for(int i = 0 ; i <= matrix.length/2 ; i++){
            for(int j = i ; i + j < matrix.length - 1 ; j ++)
                rotate(matrix, i , j, 4);
        }
    }
    public int rotate(int[][] matrix, int i, int j, int count) {
        int p = matrix[i][j];
        if(count>0)
            matrix[i][j] = rotate(matrix, matrix.length - 1 - j, i, --count);
        return p;
    }
}
